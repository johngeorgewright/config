{
  "name": "@targetd/client",
  "version": "2.0.0",
  "description": "Using results from @targetd/server",
  "main": "dist/index.js",
  "scripts": {
    "build": "yarn clean && yarn tsc && yarn rimraf 'dist/**/?(__tests__|__mocks__|__setup__|*.test.*)'",
    "clean": "yarn rimraf dist",
    "start": "yarn tsc --watch --preserveWatchOutput",
    "release": "yarn semantic-release -e semantic-release-monorepo",
    "test": "yarn jest --passWithNoTests"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/johngeorgewright/config/issues"
  },
  "homepage": "https://github.com/johngeorgewright/config#readme",
  "devDependencies": {
    "@semantic-release/commit-analyzer": "9.0.2",
    "@semantic-release/exec": "6.0.3",
    "@semantic-release/git": "10.0.1",
    "@semantic-release/github": "8.1.0",
    "@semantic-release/release-notes-generator": "11.0.2",
    "@targetd/api": "workspace:*",
    "@types/jest": "29.5.2",
    "@types/node": "18.16.16",
    "jest": "29.5.0",
    "rimraf": "5.0.1",
    "semantic-release": "21.0.2",
    "semantic-release-monorepo": "7.0.5",
    "ts-jest": "29.1.0",
    "typescript": "5.1.3",
    "zod": "3.21.4"
  },
  "dependencies": {
    "ts-toolbelt": "9.6.0",
    "tslib": "2.5.2"
  },
  "peerDependencies": {
    "@targetd/api": ">=4.0.0",
    "zod": "^3.20.0"
  }
}
